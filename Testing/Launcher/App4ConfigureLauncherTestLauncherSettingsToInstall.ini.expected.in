[General]
launcherNoSplashScreen=false
launcherSplashImagePath=
launcherSplashScreenHideDelayMs=
additionalLauncherHelpShortArgument=-h
additionalLauncherHelpLongArgument=--help
additionalLauncherNoSplashArguments=--no-splash

launcherLoadEnvironment=0
additionalSettingsFilePath=<APPLAUNCHER_DIR>/additional-settings-installed.ini
additionalSettingsExcludeGroups=General,Application

[Application]
path=<APPLAUNCHER_DIR>/bin/App4ConfigureLauncherTest@CMAKE_EXECUTABLE_SUFFIX@
arguments=--foo bar
name=App4ConfigureLauncherTest
revision=0.1
organizationDomain=com.kitware
organizationName=Kitware

[ExtraApplicationToLaunch]


[Environment]
additionalPathVariables=PYTHONPATH,QT_PLUGIN_PATH

[LibraryPaths]
1\path=<APPLAUNCHER_DIR>/lib
size=1

[Paths]
1\path=<APPLAUNCHER_DIR>/cow
2\path=<APPLAUNCHER_DIR>/pig
3\path=<APPLAUNCHER_SETTINGS_DIR>/donkey
size=3

[EnvironmentVariables]
BAR=ASSOCIATION
PLACEHOLDER=install-<APPLAUNCHER_DIR>-<APPLAUNCHER_NAME>



[PYTHONPATH]
1\path=<APPLAUNCHER_DIR>/lib/python/site-packages
size=1

[QT_PLUGIN_PATH]
1\path=<APPLAUNCHER_DIR>/libexec/qt
size=1
