[General]
launcherNoSplashScreen=false
launcherSplashImagePath=
launcherSplashScreenHideDelayMs=
launcherSplashScreenIgnoreOutput=false
additionalLauncherHelpShortArgument=-h
additionalLauncherHelpLongArgument=--help
additionalLauncherNoSplashArguments=--no-splash

launcherLoadEnvironment=0
additionalSettingsFilePath=@ctkapplauncher_dir@/additional-settings-build.ini
additionalSettingsExcludeGroups=General,Application

[Application]
path=<APPLAUNCHER_DIR>/bin/@launcher_build_subdir@/App4ConfigureLauncherTest@CMAKE_EXECUTABLE_SUFFIX@
arguments=--foo bar
name=App4ConfigureLauncherTest
revision=0.1
organizationDomain=com.kitware
organizationName=Kitware

[ExtraApplicationToLaunch]


[Environment]
additionalPathVariables=PYTHONPATH,QT_PLUGIN_PATH

[LibraryPaths]
1\path=<APPLAUNCHER_DIR>/build/lib
size=1

[Paths]
1\path=<APPLAUNCHER_DIR>/dog
2\path=<APPLAUNCHER_DIR>/cat
3\path=<APPLAUNCHER_SETTINGS_DIR>/frog
size=3

[EnvironmentVariables]
BAR=NONE
PLACEHOLDER=build-<APPLAUNCHER_DIR>-<APPLAUNCHER_NAME>



[PYTHONPATH]
1\path=<APPLAUNCHER_DIR>/build/lib/python/site-packages
size=1

[QT_PLUGIN_PATH]
1\path=<APPLAUNCHER_DIR>/build/libexec/qt
size=1
