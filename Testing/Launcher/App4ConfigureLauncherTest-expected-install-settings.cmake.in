set(CTKAPPLAUNCHER_HELP_SHORT_ARG "-h")
set(CTKAPPLAUNCHER_HELP_LONG_ARG "--help")
set(CTKAPPLAUNCHER_NOSPLASH_ARGS "--no-splash")
